DROP TABLE IF EXISTS MONEDAS;
CREATE TABLE MONEDAS (
    CLAVE               VARCHAR(3)  ,
    MONEDA              VARCHAR(100)    
);


ALTER TABLE MONEDAS ADD CONSTRAINT PK_MONEDAS         PRIMARY KEY (CLAVE);

ALTER TABLE MONEDAS ADD CONSTRAINT NOTNULL_MONEDAS_MONEDA CHECK (MONEDA IS NOT NULL);

ALTER TABLE MONEDAS  ADD CONSTRAINT EMPTY_MONEDAS_CLAVE CHECK (CLAVE != '');
ALTER TABLE MONEDAS  ADD CONSTRAINT EMPTY_MONEDAS_CLAVE CHECK (MONEDA != '');


